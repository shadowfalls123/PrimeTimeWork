    "clean": "rm -rf build dist node_modules/.vite",
    "deps:check": "npm outdated",
    "deps:update": "npm update"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
